{
  "package": "com.example.demo",
  "name": "demo",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1100,
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.device"
    },
    {
      "name": "system.storage"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "template/official": "demo-template",
  "config": {
    "logLevel": "debug"
  },
  "router": {
    "entry": "pages/tabbar",
    "pages": {
      "pages/tabbar": {
        "component": "index"
      },
      "pages/demo": {
        "component": "index"
      },
      "pages/demo/request": {
        "component": "index"
      },
      "pages/demo/navbar": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "pages": {
      "pages/tabbar": {
        "titleBar": false,
        "statusBarImmersive": true,
        "statusBarBackgroundOpacity": 0
      },
      "pages/demo": {
        "titleBarText": "案例"
      },
      "pages/demo/request": {
        "titleBarText": "请求接口"
      },
      "pages/demo/navbar": {
        "titleBarText": "自定义 NavBar、TabBar",
        "titleBar": false,
        "statusBarImmersive": true,
        "statusBarBackgroundOpacity": 0
      }
    }
  }
}
